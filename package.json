        "express": "~4.16.1",
        "hbs": "^4.1.2",
        "http-errors": "~1.6.3",
        "jsonwebtoken": "^8.5.1",
        "migrate": "^1.7.0",
        "mongoose": "^5.13.2",
        "morgan": "~1.9.1",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0"
    },
    "devDependencies": {
        "eslint": "^7.30.0",
        "nodemon": "^2.0.12"
    }
