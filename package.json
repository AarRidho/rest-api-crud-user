    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "dev": "nodemon  ./bin/www",
        "migrate:create": "migrate create --template-file ./src/migrations/utils/template.js --migrations-dir=\"./src/migrations/migrations\"",
